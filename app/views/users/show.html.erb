<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<% unless @user.title.blank? %>
  <p>
    <strong>Title:</strong>
    <%= @user.title %>
  </p>
<% end %>

<% unless @user.bio.blank? %>
  <p>
    <strong>Bio:</strong>
    <%= @user.bio || "" %>
  </p>
<% end %>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>

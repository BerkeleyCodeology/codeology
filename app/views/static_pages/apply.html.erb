        <div class="column is-10 has-text-centered">
          <h2 class="subtitle is-6 has-text-black">
          <br>
          Until next time! :)
          </h2>
        </div>
        <div class="column is-4">
          <div class="card">
            <div class="card-image">
              <figure class="image">
              <%= image_tag("cat_1.jpg", :alt=>"placeholder officer image", :class=>"") %>
              </figure>
            </div>
          </div>
        </div>
      </div>

    </div>
